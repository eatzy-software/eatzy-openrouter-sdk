{
  "name": "eatzy/openrouter-php-sdk",
  "description": "Production-ready framework-agnostic PHP SDK for OpenRouter API with Laravel integration",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "openrouter",
    "ai",
    "llm",
    "chatgpt",
    "gpt",
    "laravel"
  ],
  "authors": [
    {
      "name": "Eatzy Team",
      "email": "team@eatzy.co.uk"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.8",
    "psr/http-message": "^1.1|^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0",
    "pestphp/pest": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "OpenRouterSDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenRouterSDK\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OpenRouterSDK\\Laravel\\ServiceProvider\\OpenRouterServiceProvider"
      ],
      "aliases": {
        "OpenRouter": "OpenRouterSDK\\Laravel\\Facade\\OpenRouter"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "test-unit": "vendor/bin/pest tests/Unit",
    "test-integration": "vendor/bin/pest tests/Integration",
    "test-coverage": "vendor/bin/pest --coverage",
    "test-coverage-html": "vendor/bin/pest --coverage --min=80 --coverage-html coverage-report"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
